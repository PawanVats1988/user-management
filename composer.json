{
    "name": "Saicosys/UserManagement",
    "description": "Saicosys User Management plugin for CakePHP 4",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "UserManagement\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserManagement\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
